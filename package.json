{
  "name": "react-templae",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js --mode=development",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js --mode=production",
    "lint": "eslint --ext .tsx,.ts,.js,.jsx --fix src/",
    "debugStart": "cross-env NODE_ENV=development node --inspect-brk ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./config/webpack.config.js --mode=development",
    "debugBuild": "cross-env NODE_ENV=production node --inspect-brk ./node_modules/webpack/bin/webpack.js --config ./config/webpack.config.js --mode=production",
    "prepareHusky": "husky install",
    "commit": "git-cz",
    "gitPullAll": "git pull && git pull gitee",
    "gitPushAll": "git push && git push gitee",
    "startAll": "npm-run-all --parallel startQ:*",
    "startQ:main": "npm start",
    "startQ:sub-vue": "cd subApps/sub-vue && npm start",
    "startQ:sub-react": "cd subApps/sub-react && npm start",
    "buildAll": "cross-env BUNDLE_NO_ANALZER=true yarn build && npm-run-all --parallel buildQ:*",
    "buildQ:sub-react": "cd subApps/sub-react && npm run build && mkdir -p ../../build/subapp/sub-react && cp -r ./build/* ../../build/subapp/sub-react/",
    "buildQ:sub-vue": "cd subApps/sub-vue && npm run build && mkdir -p ../../build/subapp/sub-vue && cp -r ./dist/* ../../build/subapp/sub-vue/",
    "installAll": "npm-run-all --serial installQ:*",
    "installQ:main": "yarn",
    "installQ:sub-vue": "cd subApps/sub-vue && yarn",
    "installQ:sub-react": "cd subApps/sub-react && yarn"
  },
  "author": "",
  "license": "ISC",
